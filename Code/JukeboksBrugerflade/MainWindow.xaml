<Window x:Class="JukeboksBrugerflade.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Spawner 1.0" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="100"/>
            <RowDefinition Height="100"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="100"/>
            <ColumnDefinition Width="100"/>
        </Grid.ColumnDefinitions>
        <Button Content="Spawn Jukeboks" Click="butJukeboks" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="2"/>
        <Button Content="Spawn Klient" Click="butKlient" Grid.Row="2" Margin="0,11.41,0,2"  HorizontalAlignment="Left"  VerticalAlignment="Center" Width="100"/>

        <Label Content="Label" Grid.Row="1" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Label Content="Label" Grid.Row="2" Grid.Column="1" Margin="0,50,0,0" HorizontalAlignment="Left" VerticalAlignment="Top"/>
    </Grid>
</Window>
 